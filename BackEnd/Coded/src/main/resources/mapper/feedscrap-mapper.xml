<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="FeedScrap">
  	<insert id="insertFeedScrap">
  		insert into feedscrap values(feedscrap.nextval,${userNo},${feedPostId})
  	</insert>
  	<delete id="deleteFeedScrap">
  		delete feedscrap where feedlikeid = ${feedLikeId} and userno = ${userNo}
  	</delete>
  </mapper>