<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="AddressCoord">
	<select id="selectDistinctAddress1" resultType="String">
		SELECT DISTINCT Address1 FROM AddressCoord
	</select>
	<select id="selectScopedAddress2" resultType="String">
		SELECT Address2 FROM AddressCoord WHERE Address1=#{address1}
	</select>
</mapper>